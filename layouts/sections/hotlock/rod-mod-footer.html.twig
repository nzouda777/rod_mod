{% if content %}
<footer class="footer-handler">
    <div class="footer-elements row">
        <div class="col-lg-3 col-md-5 first-column">
         <div {{ attributes.addClass("img-logo")}}>
                {% if content.company_logo.layout_builder_add_block %}
                    {{ content.company_logo }}
                {% endif %}
              </div>
            <div class="element">
                <span class="icon1">
                    <i>
                        <svg xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="img" width="15" height="15" preserveAspectRatio="xMidYMid meet" viewBox="0 0 1024 1024"><path fill="currentColor" d="M946.5 505L534.6 93.4a31.93 31.93 0 0 0-45.2 0L77.5 505c-12 12-18.8 28.3-18.8 45.3c0 35.3 28.7 64 64 64h43.4V908c0 17.7 14.3 32 32 32H448V716h112v224h265.9c17.7 0 32-14.3 32-32V614.3h43.4c17 0 33.3-6.7 45.3-18.8c24.9-25 24.9-65.5-.1-90.5z"/></svg>
                    </i>
                </span>
                {% if content.company_addr %}
                    <div {{ region_attributes.company_addr.addClass("location")}}>
                        {{ content.company_addr }}
                    </div>
                {% endif %}
               
            </div>
            <div class=" element">
                <span class="icon">
                    <i>
                        <svg xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="img" width="15" height="15" preserveAspectRatio="xMidYMid meet" viewBox="0 0 1024 1024"><path fill="currentColor" d="M928 160H96c-17.7 0-32 14.3-32 32v640c0 17.7 14.3 32 32 32h832c17.7 0 32-14.3 32-32V192c0-17.7-14.3-32-32-32zm-40 110.8V792H136V270.8l-27.6-21.5l39.3-50.5l42.8 33.3h643.1l42.8-33.3l39.3 50.5l-27.7 21.5zM833.6 232L512 482L190.4 232l-42.8-33.3l-39.3 50.5l27.6 21.5l341.6 265.6a55.99 55.99 0 0 0 68.7 0L888 270.8l27.6-21.5l-39.3-50.5l-42.7 33.2z"/></svg>
                    </i>
                </span>
                 {% if content.company_mail %}
                    <div {{ region_attributes.company_mail.addClass("email")}}>
                        {{ content.company_mail }}
                    </div>
                {% endif %}
            </div>
            <div class=" element">
                <span class="icon">
                    <i>
                        <svg xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="img" width="15" height="15" preserveAspectRatio="xMidYMid meet" viewBox="0 0 16 16"><g fill="currentColor"><path d="M11 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h6zM5 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H5z"/><path d="M8 14a1 1 0 1 0 0-2a1 1 0 0 0 0 2z"/></g></svg>
                    </i>
                </span>
                <div class="phone-number">
                 {% if content.company_phone %}
                    <a {{ region_attributes.company_phone.addClass("link")}}>
                        {{ content.company_phone }}
                    </a>
                {% endif %}
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-5 second-column">
            <div class="head-element">
                <p>Links</p>
            </div>
            <div class="links">
                <div class="link">
                {% if content.link_A %}
                    <a href="#" {{ region_attributes.link_A.addClass("linker")}}>
                        {{ content.link_A }}
                    </a>
                {% endif %}
                </div>
                <div class="link">
                {% if content.link_B %}
                    <a href="#" {{ region_attributes.link_B.addClass("linker")}}>
                        {{ content.link_B }}
                    </a>
                {% endif %}
                </div>
                <div class="link">
                {% if content.link_C %}
                    <a href="#" {{ region_attributes.link_C.addClass("linker")}}>
                        {{ content.link_C }}
                    </a>
                {% endif %}
                </div>
                <div class="link">
                {% if content.link_D %}
                    <a href="#" {{ region_attributes.link_D.addClass("linker")}}>
                        {{ content.link_D }}
                    </a>
                {% endif %}
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-5 third-column ">
            <div class="head-element">
                <p>Navigation</p>
            </div>
            <div class="links">
                <div class="link">
                {% if content.link_E %}
                    <a href="#" {{ region_attributes.link_E.addClass("linker")}}>
                        {{ content.link_E }}
                    </a>
                {% endif %}
                </div>
                <div class="link">
                {% if content.link_F %}
                    <a href="#" {{ region_attributes.link_F.addClass("linker")}}>
                        {{ content.link_F }}
                    </a>
                {% endif %}
                </div>
                <div class="link">
                {% if content.link_E %}
                    <a href="#" {{ region_attributes.link_E.addClass("linker")}}>
                        {{ content.link_E }}
                    </a>
                {% endif %}
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-5 forth-column">
            <div class="head-element">
                <p>Suscribe</p>
            </div>
            <div class="description">
                <p>
                    Enter your email subscribe to our news and updates by email.
                </p>
            </div>
            <div class="email-form">
                <input type="email" class="mail" placeholder="Your Email" name="" id="">
                <button type="submit" value="SIGN UP" class="send">SIGN UP</button>
            </div>
        </div>
    </div>
    <div class="copyright text-center">
        <p class="elements"><a href="#" class="f-link">WB-universeThemes</a> Â© 2022. All rights reserved.</p>
    </div>
</footer>
{% endif %}